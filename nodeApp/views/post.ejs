<html>
    <body>
        <h2>
            <% page_title %>
        </h2>
        <table border="1" cellpadding="7" cellspacing="7">
            <tr>
                <th>Post ID</th>
                <th>Post Title</th>
                <th width="150px">Post Body</th>
                <th>User Name</th>
                <th>UserName</th>
                <th>User Email</th>
                <th>User Phone</th>
            </tr>
            <% if(data.length){ for(var i = 0; i < data.length; i++) { %>
            <tr>
                <td><%=data[i].id%></td>
                <td><%=data[i].title%></td>
                <td><%=data[i].body%></td>
                <td><%=data[i].dataObject.name%></td>
                <td><%=data[i].dataObject.username%></td>
                <td><%=data[i].dataObject.email%></td>
                <td><%=data[i].dataObject.phone%></td>
            </tr>
            <% } }else{ %>
            <tr>
                <td colspan="3">No Post Data</td>
            </tr>
            <% } %>
        </table>
    </body>
</html>
